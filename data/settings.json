ocument.getElementById('msg');\n\n\nfunction show(text, ok){\nmsg.textContent = text;\nmsg.style.color = ok ? 'green' : 'crimson';\n}\n\n\nbtn.addEventListener('click', function(){\nconst v = pwd.value.trim();\nif(!v){ show('請輸入密碼', false); return; }\nif(v === CORRECT){\nshow('密碼正確，歡迎！', true);\n// 成功後可在此導向或顯示保護內容\n// location.href = '/private.html';\n} else {\nshow('密碼錯誤', false);\n}\n});\n\n\nclr.addEventListener('click', function(){ pwd.value = ''; pwd.focus(); msg.textContent = ''; });\n\n\n// 支援按 Enter 鍵提交\npwd.addEventListener('keydown', function(e){ if(e.key === 'Enter') btn.click(); });\n\n\n// 預防密碼被輸出到 console（提示用）\nconsole.log = function(){};\n})();\n</script>\n</body>\n</html>","footerContent":"<div class=\"dark-white\">\n  <div>共收錄${total}個網站</div>\n  <div>Copyright © 2018-${year} ${hostname}, All Rights Reserved</div>  \n</div>","showThemeToggle":true,"logo":"","darkLogo":"","lightDocTitle":"","lightCardStyle":"standard","lightOverType":"overflow","lightFooterHTML":"","simThemeImages":[{"src":"https://gcore.jsdelivr.net/gh/xjh22222228/public@gh-pages/nav/banner1.jpg","url":"https://github.com/xjh22222228/nav"},{"src":"https://gcore.jsdelivr.net/gh/xjh22222228/public@gh-pages/nav/banner2.jpg","url":"https://github.com/xjh22222228/nav"}],"simThemeDesc":"这里收录多达 <b>${total}</b> 个优质网站， 助您工作、学习和生活","simCardStyle":"original","simOverType":"overflow","simThemeHeight":0,"simThemeAutoplay":true,"simDocTitle":"","simTitle":"","simFooterHTML":"","superCardStyle":"column","superOverType":"overflow","superFooterHTML":"","superDocTitle":"","superTitle":"","superImages":[{"src":"https://gcore.jsdelivr.net/gh/xjh22222228/nav-image@image/nav-1717494364392-ad.jpg","url":"https://haokawx.lot-ml.com/Product/index/454266"},{"src":"https://gcore.jsdelivr.net/gh/xjh22222228/public@gh-pages/img/10.png","url":""}],"lightImages":[{"src":"https://img.pikbest.com/back_our/bg/20200709/bg/029dc47e1276b_353069.jpg!bwr800","url":"https://www.nicegarden.com.tw/"}],"sideTitle":"","sideDo